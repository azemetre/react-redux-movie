{
  "name": "react-redux-movie",
  "version": "1.0.0",
  "description": "simple movie detail site written using react and redux",
  "main": "index.js",
  "scripts": {
    "server": "nodemon server/index.js",
    "wp:server": "concurrently \"webpack --watch --config webpack.config.js\" \"webpack-dev-server\"",
    "wp:watch": "./node_modules/.bin/webpack --watch --config webpack.config.js",
    "dev": "npm run server & npm run wp:watch",
    "test": "node_modules/.bin/mocha --trace-warnings --timeout 15000",
    "tests:e2e": "npm run test tests/e2e/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azemetre/react-redux-movie"
  },
  "keywords": [
    "react",
    "redux",
    "movies",
    "js",
    "javascript",
    "babel",
    "webpack"
  ],
  "author": "Aaron Zemetres (http://azemetre.me)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^4.0.7",
    "react-router": "^3.0",
    "redux": "^3.5.0",
    "redux-actions": "^1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^1.0",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "mocha": "^5.1.1",
    "react-test-renderer": "^16.3.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "style-loader": "^0.21.0",
    "webpack": "^1.0",
    "webpack-dev-server": "^1.0"
  }
}
